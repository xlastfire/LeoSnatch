{
  "manifest_version": 3,
  "name": "LeoSnatch",
  "version": "1.0",
  "description": "Snatch generated images from Leonardo.ai automatically with prompts.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
